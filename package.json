{
  "name": "mangaupdates-rss",
  "version": "0.1.0",
  "author": "IntermittentlyRupert",
  "description": "Generates a MangaUpdates RSS feed for your lists.",
  "license": "MIT",
  "type": "module",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "."
  },
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "node --inspect=9229 --trace-warnings --experimental-specifier-resolution=node --require dotenv/config dist/index.js",
    "watch": "nodemon --ext ts,js,json --ignore *.d.ts -w src --exec \"npm run build && npm run dev\"",
    "build": "rimraf ./dist && babel --extensions \".ts,.js\" --out-dir dist --copy-files src",
    "lint": "npm run lint:style && npm run lint:quality && npm run lint:types",
    "lint:style": "prettier --check src",
    "lint:quality": "eslint --max-warnings 0 src",
    "lint:types": "tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "http-cookie-agent": "^1.0.4",
    "morgan": "^1.10.0",
    "node-fetch": "^3.1.0",
    "tough-cookie": "^4.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/eslint-plugin": "^7.16.5",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/d3-dsv": "^3.0.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.18.3",
    "@types/tough-cookie": "^4.0.1",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  }
}
